{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Run Test Debugger",
            "runtimeExecutable": "npm",
            "runtimeArgs": [
                "run",
                "test:debug"
            ],
            "cwd": "${fileDirname}",
            "skipFiles": [
                "<node_internals>/**/**",
                "node_modules/**/**",
            ],
            "console": "integratedTerminal"
        }
    ],
    "compounds": []
}